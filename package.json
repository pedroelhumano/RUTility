    "test:coverage": "npx jest --coverage --collectCoverageFrom='src/**/*.ts'",
    "test:lint": "eslint --fix",
    "test": "npm run test:unit & npm run test:coverage & npm run test:lint",
    "build-browser": "tsc --project tsconfig.browser.json",
